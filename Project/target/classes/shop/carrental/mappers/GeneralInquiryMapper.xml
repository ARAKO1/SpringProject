<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="shop.carrental.mappers.GeneralInquiryMapper">

	<insert id="register">
		<![CDATA[
			INSERT INTO customer_inquiry
				(seq,email,title,content,privacy)
			VALUES
				(seq_customer_inquiry.NEXTVAL,#{email},#{title},#{content},#{privacy})
		]]>
	</insert>

</mapper>